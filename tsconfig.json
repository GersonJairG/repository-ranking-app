  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": ["*"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "contexts/*": ["contexts/*"],
      "data/*": ["data/*"],
      "gql/*": ["gql/*"],
      "hooks/*": ["hooks/*"],
      "interfaces/*": ["interfaces/*"],
